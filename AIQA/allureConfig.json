{
  "allure": {
    "directory": "allure-results",
    "links": [
      "https://github.com/annette-medvedeva/AIQA/issues/{}"
    ],
    "categories": [
      {
        "name": "Authentication Failures", 
        "messageRegex": ".*login.*|.*auth.*|.*credential.*",
        "traceRegex": ".*LoginPage.*|.*AuthenticationException.*"
      },
      {
        "name": "Product Management Issues",
        "messageRegex": ".*product.*|.*cart.*|.*inventory.*",
        "traceRegex": ".*ProductsPage.*|.*CartPage.*"
      },
      {
        "name": "Browser Issues",
        "messageRegex": ".*browser.*|.*page.*|.*navigation.*",
        "traceRegex": ".*BrowserConfiguration.*|.*NavigationException.*"
      },
      {
        "name": "Infrastructure Problems",
        "messageRegex": ".*timeout.*|.*connection.*|.*network.*"
      }
    ],
    "environment": [
      {
        "name": "Framework",
        "value": ".NET 8"
      },
      {
        "name": "Language", 
        "value": "C# 12.0"
      },
      {
        "name": "Test Framework",
        "value": "NUnit 4.x"
      },
      {
        "name": "Browser Automation",
        "value": "Microsoft Playwright"
      },
      {
        "name": "Application Under Test",
        "value": "Saucedemo"
      }
    ]
  }
}